{
    "name": "@aws-toolkit/telemetry",
    "version": "0.0.1",
    "publisher": "amazonwebservices",
    "description": "A library used for the AWS Toolkit for VSCode to provide telemetry",
    "main": "index.js",
    "bin": {
        "generate-telemetry": "./out/index.js"
    },
    "scripts": {
        "build": "node ./scripts/setUpPackage.js && tsc -p ./",
